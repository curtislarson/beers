{
  "lineWidth": 120,
  "indentWidth": 2,
  "newLineKind": "auto",
  "useTabs": false,
  "incremental": true,
  "typescript": {
    "arrowFunction.useParentheses": "force",
    "quoteStyle": "alwaysDouble"
  },
  "json": {},
  "markdown": {
    "lineWidth": 80
  },
  "dockerfile": {},
  "includes": [
    "**/*.{ts,tsx,jsx,cjs,mjs,mts,cts,json,md,mdx,yml,yaml,jsonc,dockerfile,conf}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/node",
    "**/.deno",
    "**/vendor",
    "**/packages-experiments/**",
    "**/dist",
    "**/.gen",
    "pnpm-lock.yaml",
    "pnpm-workspace.yaml",
    "lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.77.0.wasm",
    "https://plugins.dprint.dev/json-0.16.0.wasm",
    "https://plugins.dprint.dev/markdown-0.14.1.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.0.wasm",
    "https://plugins.dprint.dev/prettier-0.13.0.json@dc5d12b7c1bf1a4683eff317c2c87350e75a5a3dfcc127f3d5628931bfb534b1"
  ]
}
